write.csv(Flux, "20180706_CultureFluxData_Processed.csv")

head(Flux)
unique(Flux$Species)

Flux$Species<-ordered(Flux$Species, levels=c("Gloeophyllum trabeum", "Postia placenta", "Schizophyllum commune", 
                                             "Pycnoporus cinnabarinus", "Trametes hirsuta", NA))

pick1<-Flux$MesocosmTreatment=="WoodChip" 
WoodchipMeans<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], mean, na.rm=T)
WoodchipUpper<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], quantile, 0.975, na.rm=T)
WoodchipLower<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], quantile, 0.025, na.rm=T)

pick1<-Flux$MesocosmTreatment=="NoChip" 
NochipMeans<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], mean, na.rm=T)
NochipUpper<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], quantile, 0.975, na.rm=T)
NochipLower<-tapply(Flux[pick1,"CO2Flux_ug"], Flux[pick1,"Species"], quantile, 0.025, na.rm=T)


Means<-rbind(as.vector(WoodchipMeans), as.vector(NochipMeans))
Upper<-rbind(as.vector(WoodchipUpper), as.vector(NochipUpper))
Lower<-rbind(as.vector(WoodchipLower), as.vector(NochipLower))

par(mar=c(5.5,4,1,4))
barx<-barplot(rbind(as.vector(WoodchipMeans), as.vector(NochipMeans)), beside=T, ylim=c(0,4.5), col=c("#0072B2","#e79f00"))
position<-barx
# axis(side=2, tck=0.02, labels=NA)
# axis(side=2, lwd=0, line = -.4, las=1, cex.axis=1)
mtext(side=2, expression(paste("",CO[2]," flux")), line=2.75, cex=1.25, col="black")
mtext(side=2, expression(paste("", mu, "umol ",CO[2]," " , jar^-1," ",min^-1,"", sep="")), line=1.75, cex=1, col="black")
errbar(position, Means,Upper, Lower, cap=0.015, xaxt="n", add=T, col=c("#0072B2","#e79f00"), pch="." )
legend("topright", c("Birch Wafer + Soil Matrix","Soil Matrix Only"), col=c("#0072B2","#e79f00"), pch=c(16,16), bty="n")
box()
par(mgp=c(0,0.25,0))
axis(1, at = c(2,5,8,11,14), labels = c("Gloeophyllum\ntrabeum", "Postia\nplacenta", "Schizophyllum\ncommune", "Pycnoporus\ncinnabarinus", "Trametes\nhirsuta"), 
     las=2, tick = F, cex=0.6, font=3, line=0)

