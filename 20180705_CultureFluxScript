Jar35_slope<-coef(lm(unlist(Flux[35,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol")])~
                       unlist(Flux[35,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))[2] 
Jar35_R2<-summary(lm(unlist(Flux[35,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )])~
                       unlist(Flux[35,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))$r.squared 
