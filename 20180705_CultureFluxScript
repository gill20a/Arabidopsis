setwd("~/Dropbox/UMN/Arabidopsis experiment/Flux measurements/") #Set network drive, will have to change for your own computer
Flux<-read.csv("20180705_Culture_FluxData.csv", sep=",", header=T) #Name your data file to call it later in the code
head(Flux)
# install.packages("Hmisc")
# install.packages("Matrix")
library(Hmisc)
library(Matrix)

##Calibration Data
KnownConcentrations<-c(0, 645, 1025, 10000)


##Change format of time stamps
Flux$Time_1<-as.POSIXct(as.character(Flux$Time_1), format="%H:%M:%S")
Flux$Time_2<-as.POSIXct(as.character(Flux$Time_2), format="%H:%M:%S")
Flux$Time_3<-as.POSIXct(as.character(Flux$Time_3), format="%H:%M:%S")
Flux$Time_4<-as.POSIXct(as.character(Flux$Time_4), format="%H:%M:%S")

#Calculate amount of time elapsed during measurements
Flux$TimeInt_1<-as.numeric(as.character(difftime(Flux$Time_1, Flux$Time_1))) #Should be a vector full of zeros
Flux$TimeInt_2<-as.numeric(as.character(difftime(Flux$Time_2, Flux$Time_1)))
Flux$TimeInt_3<-as.numeric(as.character(difftime(Flux$Time_3, Flux$Time_1)))
Flux$TimeInt_4<-as.numeric(as.character(difftime(Flux$Time_4, Flux$Time_1)))


#Read Concentrations. Will need to update these values during for measurement period. Replace any obviously strange values with NA.
LowStd<-c(639, 460, 598)
MedStd<-c(1039, 812, 908)
HighStd<-c(7717, 8094, 737)
ReadConcentrations<-c(0, mean(LowStd, na.rm=T), mean(MedStd, na.rm=T), mean(HighStd, na.rm=T))

CalibrationSlope<-coef(lm(ReadConcentrations~KnownConcentrations))[2] 
CalibrationR2<-summary(lm(ReadConcentrations~KnownConcentrations))$r.squared 

Flux$CO2_1_umol<-Flux$CO2_1_ppm/CalibrationSlope*(0.4/(0.082057*295))
Flux$CO2_2_umol<-Flux$CO2_2_ppm/CalibrationSlope*(0.4/(0.082057*295))
Flux$CO2_3_umol<-Flux$CO2_3_ppm/CalibrationSlope*(0.4/(0.082057*295))
Flux$CO2_4_umol<-Flux$CO2_4_ppm/CalibrationSlope*(0.4/(0.082057*295))


#########################Jar1 Flux data
plot(unlist(Flux[1,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")]), 
     unlist(Flux[1,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )]))
Flux[1,"TimeInt_4"]<-NA
Flux[1,"CO2_4_umol"]<-NA
Jar1_slope<-coef(lm(unlist(Flux[1,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol")])~
                      unlist(Flux[1,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))[2] 
Jar1_R2<-summary(lm(unlist(Flux[1,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )])~
                      unlist(Flux[1,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))$r.squared 
Jar1_slope
Jar1_R2

##
plot(unlist(Flux[2,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")]), 
     unlist(Flux[2,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )]))
Flux[2,"TimeInt_3"]<-NA
