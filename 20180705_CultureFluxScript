plot(unlist(Flux[21,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")]), 
     unlist(Flux[21,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )]))
Jar21_slope<-coef(lm(unlist(Flux[21,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol")])~
                       unlist(Flux[21,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))[2] 
Jar21_R2<-summary(lm(unlist(Flux[21,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )])~
                       unlist(Flux[21,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))$r.squared 
Jar21_slope
Jar21_R2

