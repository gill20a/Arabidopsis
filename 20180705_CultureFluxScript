     unlist(Flux[48,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )]))
abline(lm(unlist(Flux[48,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )])~
            unlist(Flux[48,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])), col="red")
Jar48_slope<-coef(lm(unlist(Flux[48,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol")])~
                       unlist(Flux[48,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))[2] 
Jar48_R2<-summary(lm(unlist(Flux[48,c("CO2_1_umol","CO2_2_umol","CO2_3_umol","CO2_4_umol" )])~
                       unlist(Flux[48,c("TimeInt_1","TimeInt_2","TimeInt_3","TimeInt_4")])))$r.squared 
Jar48_slope
Jar48_R2

